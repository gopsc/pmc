[Unit]
Description=TxL.phs
After=network.target
Wants=network.target

[Service]
User=root
Group=root
WorkingDirectory=/root/___TxL/TestSuits/phs
ExecStart=bash /root/___TxL/TestSuits/phs/_run.sh
Restart=always
RestartSec=10
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=mozart-python-host
Environment=PYTHONUNBUFFERED=1

[Install]
WantedBy=multi-user.target
